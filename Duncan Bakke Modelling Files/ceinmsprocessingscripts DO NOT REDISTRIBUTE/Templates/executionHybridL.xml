<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<execution xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="execution.xsd">
  <NMSmodel>
     <type>
        <hybrid>
            <alpha> 1 </alpha>
            <beta> 0 </beta>
            <gamma> 50 </gamma>
            <trackedMuscles/>
            <predictedMuscles> psoas_l iliacus_l</predictedMuscles>
            <algorithm>
                <simulatedAnnealing>
                    <noEpsilon>4</noEpsilon>
                    <rt>0.3</rt>
                    <T>20</T>
                    <NS>15</NS>
                    <NT>5</NT>
                    <epsilon>1.E-3</epsilon>
                    <maxNoEval>200000</maxNoEval>
                </simulatedAnnealing>
            </algorithm>
        </hybrid>
     </type>

     <tendon>
          <equilibriumElastic>
            <tolerance>0.000000001</tolerance>
          </equilibriumElastic>
     </tendon>

     <activation>
          <exponential/>
     </activation>
  </NMSmodel>

  <online/>

  <elaboratedDoFs>
  </elaboratedDoFs>

</execution>